{
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.organizeImports": "always"
	},
	"typescript.tsdk": "./node_modules/typescript/lib",
	"github-actions.workflows.pinned.workflows": [".github/workflows/workers-ai-catalog.yml", ".github/workflows/azure-catalog.yml", ".github/workflows/test.yml", ".github/workflows/changeset-release.yml"],
	"github-actions.workflows.pinned.refresh.enabled": true,
	"yaml.schemas": {
		"https://json.schemastore.org/dependabot-2.0.json": "${workspaceFolder}/.github/dependabot.yml"
	},
	"json.schemas": [
		{
			"fileMatch": ["**/devcontainer.json"],
			"url": "https://raw.githubusercontent.com/devcontainers/spec/main/schemas/devContainer.schema.json"
		}
	]
}
